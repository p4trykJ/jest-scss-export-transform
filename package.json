        "transformer",
        "scss",
        "sass"
    ],
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.5",
        "@babel/plugin-transform-modules-commonjs": "^7.19.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.10",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
